<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.quotes2.Controllers.VerificatorController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <AnchorPane prefHeight="400.0" prefWidth="300.0" style="-fx-background-color: #fafafa;">
        <children>
            <Button fx:id="DeleteGroupQ" layoutX="53.0" layoutY="254.0" onAction="#onHelloButtonClick" prefHeight="25.0" prefWidth="193.0" text="Удалить цитаты группы" />

            <Label fx:id="welcomeText" layoutX="60.0" layoutY="20.0" prefHeight="31.0" prefWidth="193.0" text="Teachers' quotes" textAlignment="CENTER">
                <font>
                    <Font name="Book Antiqua" size="24.0" />
                </font>
            </Label>
         <Button fx:id="lookAll" layoutX="53.0" layoutY="148.0" onAction="#onHelloButtonClick" prefHeight="25.0" prefWidth="193.0" text="Посмотреть все цитаты" />
         <Button fx:id="editGroupQ" layoutX="53.0" layoutY="200.0" onAction="#onHelloButtonClick" prefHeight="25.0" prefWidth="193.0" text="Редактировать цитаты группы" />
        </children>
    </AnchorPane>
</VBox>
